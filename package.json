{
  "name": "tilebelt-wgs84",
  "version": "1.0.2",
  "description": "Simple tile utilities for WGS84 / EPSG:4326",
  "type": "module",
  "main": "dist/bundle.cjs",
  "exports": {
    "node": {
      "module": "./dist/bundle.js",
      "require": "./dist/bundle.cjs"
    },
    "default": "./dist/bundle.js",
    "script": "./dist/bundle.min.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run lint && npm run test:ci && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bwswedberg/tilebelt-wgs84"
  },
  "keywords": [
    "map",
    "tile",
    "wgs84",
    "epsg:4326"
  ],
  "author": "bwswedberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bwswedberg/tilebelt-wgs84/issues"
  },
  "homepage": "https://github.com/bwswedberg/tilebelt-wgs84",
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "jest": "^29.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.3"
  }
}
